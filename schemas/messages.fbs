namespace Protocol;

table InitializationMessage {

}

table RequestScreenshot {

}

table OCRMessage {
  key: string;
  text: string;
}

table AudioMessage {
  audio_data: [uint8];
}

union MessageData {
  InitializationMessage,
  RequestScreenshot,
  ResponseScreenShot,
  OCRMessage,
  AudioMessage
}

table Message {
  id: uint64;
  data: MessageData;
}

enum ImageFormat : ubyte {
  PNG = 0,
  JPEG = 1
}

table ResponseScreenShot {
  key: string;
  format: ImageFormat;
  image: [uint8];
}

